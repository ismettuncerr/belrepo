@model Bel.Models.PastReservationViewModel
@{
    ViewBag.Title = "PastReservationForAdmin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="banner-area relative about-banner" id="home">
    <div class="overlay overlay-bg"></div>
    <div class="container">
        <div class="row d-flex align-items-center justify-content-center">
            <div class="about-content col-lg-12">
                <h1 class="text-white">
                    Geçmiş Rezervasyonlar
                </h1>
            </div>
        </div>
    </div>
</section>
<section class="course-details-area pt-0">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 left-contents">
                <div class="jq-tab-wrapper" id="horizontalTab">
                    <div class="container">
                        <table class="table table-hover" style="cursor:pointer" id="tbldata">
                            <thead>
                                <tr>
                                    <td>Danışman Bilgisi</td>
                                    <td>Rezervasyon Tarihi</td>
                                    <td>Okul Adı</td>
                                    <td>Sınıf</td>
                                    <td>Okul Sınıfı</td>
                                    <td>Saat</td>
                                    <td>Detay</td>
                                    @*<td>Sil</td>*@
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.Reservations)
                                {
                                    <tr>
                                        <td>@item.AdvisorName</td>
                                        <td>@item.ReservationDate</td>
                                        <td>@item.UserName</td>
                                        <td>@item.MunicipalityClassName</td>
                                        <td>@item.SchoolClassName</td>
                                        <td>@item.Hour</td>
                                        <td>@Html.ActionLink("Detay", "DetailReservation", new { id = item.Id }, new { @class = "btn btn-warning" })</td>
                                        @*<td>@Html.ActionLink("Sil", "DeleteReservation", new { id = item.Id }, new { @class = "btn btn-danger" })</td>*@
                                        <!--<td>@Html.ActionLink("Detail", "DetailReservation", new { id = item.Id }, new { @class = "btn btn-warning" })</td>-->
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>           
				</div>
        </div>
    </div>
</section>
  <br />
<br />
<br />          

<link href="~/Content/paging.css" rel="stylesheet" />
<script src="~/Scripts/paging.js"></script>
<script type="text/javascript">
    $(function () {
        $('#tbldata').paging({ limit: 5 });
    })
</script>







