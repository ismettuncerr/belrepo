
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("jquery-2.2.4.min");
<section class="banner-area relative about-banner" id="home">
    <div class="overlay overlay-bg"></div>
    <div class="container">
        <div class="row d-flex align-items-center justify-content-center">
            <div class="about-content col-lg-12">
                <h1 class="text-white">
                    İletişim
                </h1>

            </div>
        </div>
    </div>
</section>

<section class="contact-page-area section-gap">
    <div class="container">
        <div class="row">
            <div class="map-wrap" style="width:100%; height: 445px;" >
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3009.5968651140643!2d28.83078431566258!3d41.03407492578535!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14caa4e1dbe3a29b%3A0x490db6e1a4c97b36!2zQmHEn2PEsWxhciBCZWxlZGl5ZXNp!5e0!3m2!1str!2str!4v1560007560435!5m2!1str!2str" width="100%" height="445" frameborder="0" style="border:0" allowfullscreen></iframe>
            </div>
            <div class="col-lg-12 d-flex flex-row address-wrap">
                <div class="single-contact-address d-flex flex-row col-md-4">
                    <div class="icon">
                        <span class="lnr lnr-home"></span>
                    </div>
                    <div class="contact-details">
                        <h5>@Model.contact.CityState</h5>
                        <p>@Model.contact.Address</p>
                    </div>
                </div>
                <div class="single-contact-address d-flex flex-row col-md-4">
                    <div class="icon">
                        <span class="lnr lnr-phone-handset"></span>
                    </div>
                    <div class="contact-details">
                        <h5>@Model.contact.Phone</h5>
                        <p>@Model.contact.PhoneDescription</p>
                    </div>
                </div>
                <div class="single-contact-address d-flex flex-row col-md-4">
                    <div class="icon">
                        <span class="lnr lnr-envelope"></span>
                    </div>
                    <div class="contact-details">
                        <h5>@Model.contact.Email</h5>
                        <p>@Model.contact.EMailDescription</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>



